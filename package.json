{
  "name": "flake-monster",
  "version": "0.4.1",
  "description": "Source-to-source test hardener that injects async delays to surface flaky tests",
  "type": "module",
  "bin": {
    "flake-monster": "bin/flake-monster.js"
  },
  "files": [
    "bin/",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "node --test test/unit/*.test.js",
    "test:integration": "node --test test/integration/*.test.js",
    "test:all": "node --test test/**/*.test.js"
  },
  "keywords": [
    "flaky-tests",
    "testing",
    "async",
    "chaos-engineering",
    "test-hardener"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/growthboot/FlakeMonster.git"
  },
  "homepage": "https://flakemonster.com",
  "bugs": {
    "url": "https://github.com/growthboot/FlakeMonster/issues"
  },
  "author": "growthboot",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "acorn": "^8.14.0",
    "acorn-walk": "^8.3.4",
    "astravel": "^0.6.1",
    "astring": "^1.9.0",
    "commander": "^13.1.0",
    "fast-glob": "^3.3.3"
  }
}
